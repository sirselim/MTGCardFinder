{
  "name": "my-card-extension",
  "displayName": "My Card Extension",
  "description": "VSCode extension for converting card names to images",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.convertCardName",
        "title": "Convert Card Name"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -watch -p ./",
    "watch": "tsc -watch",
    "test": "npm run compile && node ./out/test/runTest.js"
  },
  "dependencies": {
    "@types/node": "^14.14.37",
    "@types/mocha": "^8.2.2",
    "@types/vscode": "^1.83.0",
    "axios": "^0.21.4",
    "ts-node": "^8.10.2",
    "typescript": "^4.4.2",
    "vscode": "^1.83.0"
  },
  "devDependencies": {
  }
}
